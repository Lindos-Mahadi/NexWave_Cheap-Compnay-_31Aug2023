@model comdeeds.Models.BaseModel.ClassUserDetails
@{

    var isAuth = (bool)ViewBag.isauth;
    string inputState = "";
    inputState = isAuth ? "disabled='disabled'" : "";

    ViewBag.Title = "CompanySetup";

    List<string> Roles = (List<string>)ViewBag.LoggedInRoles;

}
<script>
    var user = {
        fname: "@Model.Firstname",
        lname: "@Model.Lastname",
        email: "@Model.Email",
        phone: "@Model.Phone",
        Tuser: "@Model.Tuser",
    };

    var activepnl = "@ViewBag.openpnl";
    var cn ="@ViewBag.cn";
</script>

<div id="titlebar">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h2>Company setup</h2>
                <nav id="breadcrumbs">

                    <ul>
                        @{
                            if (Roles.Count == 0 || (Roles.Any(x => x.ToLower() != "admin") || Roles.Any(x => x.ToLower() != "subadmin")))
                            {
                                <li><a href="/admin/dashboard" class="btn btn-default"> Go to Dashboard </a></li>
                            }
                        }
                        <li><a href="/">Home</a></li>
                        <li>Company setup</li>
                    </ul>
                </nav>
            </div>
        </div>
    </div>
</div>

<div class="container" id="bindview">
    <div class="row" id="companysetup">
        <div class="col-md-12 margin-bottom-40">
            <div class="style-2">
                <ul class="tabs-nav">
                    @{
                        if (Roles.Count == 0 || (Roles.Any(x => x.ToLower() != "admin") || Roles.Any(x => x.ToLower() != "subadmin")))
                        {
                            <li data-bind="css:tabbing()==1?'active':''"><a href="#" data-bind="click:toggletab.bind($data,1)"><i class="sl sl-icon-user"></i> User details</a></li>
                        }
                    }
                    <li data-bind="css:tabbing()==2?'active':''"><a href="#" data-bind="click:toggletab.bind($data,2)"><i class="sl sl-icon-globe"></i> Company details</a></li>
                    <li data-bind="css:tabbing()==3?'active':''"><a href="#" data-bind="click:toggletab.bind($data,3)"><i class="sl sl-icon-location"></i> Company address</a></li>
                    <li data-bind="css:tabbing()==4?'active':''"><a href="#" data-bind="click:toggletab.bind($data,4)"><i class="sl sl-icon-people"></i> Director details</a></li>
                    <li data-bind="css:tabbing()==5?'active':''"><a href="#" data-bind="click:toggletab.bind($data,5)"><i class="sl sl-icon-equalizer"></i> Share allocation</a></li>
                    <li><a href="#" onclick="return false;"><i class="sl sl-icon-eye"></i> Summary</a></li>
                </ul>

                <div class="tabs-container">
                    <input type="hidden" value="@ViewBag.cn" id="hdncname" name="hdncname" />

                    @{
                        if (Roles.Count == 0 || Roles.Any(x => x.ToLower() != "admin"))
                        {
                            <!-- applicant detail-->
                            <div class="tab-content" data-bind="css:tabbing()==1?'tab-content active':'tab-content'">

                                <form id="applicantform" class="submit-page p-0" data-bind="submit:applicantsubmit" method="post" action="@Url.Action("company-setup", "Home")">
                                    @Html.Raw(ViewBag.msg)

                                    <fieldset @inputState>
                                        <div class="clearfix"></div>
                                        <h3>User details</h3>
                                        <div class="submit-section">
                                            <div class="row with-forms">
                                                <div class="col-md-6">
                                                    <div class="row with-forms">
                                                        <div class="col-md-6">
                                                            <h5>First name</h5>
                                                            <input type="text" data-bind="value:applicant().firstname" value="@Model.Firstname" name="Firstname" id="appfname" @inputState />
                                                        </div>
                                                        <div class="col-md-6">
                                                            <h5>Last name</h5>
                                                            <input type="text" data-bind="value:applicant().lastname" name="Lastname" id="applname" @inputState />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <h5>Email</h5>
                                                    <input type="email" data-bind="value:applicant().email" name="Email" id="appemail" @inputState />
                                                </div>
                                            </div>
                                            <div class="row with-forms">
                                                @{
                                                    if (!isAuth)
                                                    {
                                                        <div class="col-md-4">
                                                            <h5>Password</h5>
                                                            <input type="password" data-bind="value:applicant().pass" name="password" id="app_pass">
                                                        </div>
                                                    }
                                                }

                                                <div class="col-md-4">
                                                    <h5>Phone</h5>
                                                    <input type="tel" data-bind="value:applicant().phone" name="Phone" maxlength="15" pattern="^[0-9\-\+]{9,15}$" id="app_phone" @inputState />
                                                </div>
                                                <div class="col-md-4">
                                                    <h5>Type of user</h5>
                                                    <select name="Tuser" id="Tuser" data-bind="value:applicant().Tuser">
                                                        <option value=">Select Type">Select Type</option>
                                                        <option value="Direct customer">Direct customer</option>
                                                        <option value="Financial Adviser">Financial Adviser</option>
                                                        <option value="Mortgage Broker">Mortgage Broker</option>
                                                        <option value="Accountant">Accountant</option>
                                                        <option value="Lawyer">Lawyer</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="divider"></div>
                                        @{
                                            if (!isAuth)
                                            {
                                                <button type="submit" class="button preview pull-right margin-bottom-15">
                                                    Continue
                                                    <i class="fa fa-arrow-circle-right"></i>
                                                </button>
                                            }
                                        }
                                    </fieldset>
                                </form>
                            </div>
                    <!-- applicant detail-->
                                            }
                    }

                    @{ if (isAuth)
                        {

                    <!-- company detail-->
                            <div class="tab-content " data-bind="css:tabbing()==2?'tab-content active':'tab-content'">
                                <form id="companyform" class="submit-page p-0" data-bind="submit:companysubmit">
                                    <h3>Company details</h3>
                                    <div class="submit-section" data-bind="foreach:company">
                                        <!-- company details-->
                                        <div class="row with-forms">
                                           
                                            <div class="col-md-12">

                                                <div class="checkboxes">
                                                    <input type="checkbox" name="chkAcnasName" id="chkAcnasName" data-bind="checked:AcnasName" />
                                                    <label for="chkAcnasName">Do you have a proposed name for your company ?</label>
                                                </div>
                                            </div>
                                            <div class="col-md-7">

                                                <h5>Company name <i class="tip" data-tip-content="Do not use words like 'bank' , 'trust' , 'Royal' , 'Incorporated' in company name.You can't use words that could mislead people about a company's activities. This includes associations with Australian government, the Royal Family, or any ex-servicemen's organisations."></i></h5>
                                                <div class="main-search-input">
                                                    <input type="text" name="companyname" id="companyname" data-bind="value:companyName,event:{blur:checkname}" placeholder="Type your company name to see if its available" />
                                                    <label for="chkAcnasName" style="padding: 12px;">PTY LTD</label>
                                                </div>
                                            </div>
                                            <div class="col-md-12" style="display:none;">
                                                <h5>Name reservation <i class="tip" data-tip-content="Is this company name identical to a registered state business name?"></i>  </h5>
                                                <div class="checkboxes margin-top-15 margin-right-15">
                                                    <input type="radio" name="rdoNameReserve" id="rdoname1" value="yes" data-bind="checked:reserve" />
                                                    <label for="rdoname1">Yes</label>
                                                </div>
                                                <div class="checkboxes margin-top-15">
                                                    <input type="radio" name="rdoNameReserve" id="rdoname2" value="no" data-bind="checked:reserve" />
                                                    <label for="rdoname2">No</label>
                                                </div>
                                                <div class="clearfix"></div>
                                            </div>
                                        </div>

                                        <div style="background-color:#fff4b6;">
                                            <div style="display:none;padding:10px 20px;" data-bind="visible:$data.reserve()=='yes'">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <h5 style="font-size:14px;">Has the company name been reserved with ASIC?</h5>
                                                        <input type="text" value="PTY.LTD" readonly="readonly" disabled="disabled" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <h5>When was your business name(s) registered?</h5>
                                                        <input type="text" value="PTY.LTD" readonly="readonly" disabled="disabled" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <h5>State business number</h5>
                                                        <input type="text" placeholder="ABN" name="abn" id="abn" data-bind="value:companyABN" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row with-forms">
                                            <div class="col-md-6">
                                                <h5> Type of company</h5>
                                                <select name="ddcomppurpose" id="ddcomppurpose" data-bind="value:purpose">
                                                    <option value="">Select</option>
                                                    <option value="PROPRIETARY (PROP)">Is this company a Proprietary company (Not a listed compnay)</option>
                                                    <option value="public">Is this company a Public company (listed company)</option>
                                                </select>
                                            </div>

                                            <div class="col-md-6">
                                                <div>
                                                    <h5>What will you use this company for ?</h5>
                                                    <select name="ddcompfor" id="ddcompfor" data-bind="value:companyuse">
                                                        <option value="">Select</option>
                                                        <option value="smsf">Trustee for a Self Managed Super</option>
                                                        <option value="A company to operate business">A company to operate business</option>
                                                        <option value="Trustee for a Self Managed Super">A Trustee company for a Trust </option>
                                                    </select>
                                                </div>

                                                <div style="background-color:#fff4b6;display:none" data-bind="visible:$data.companyuse()=='smsf'">
                                                    <div class="checkboxes margin-top-15" data-bind="validationOptions: {insertMessages: false}">
                                                        <input type="checkbox" data-bind="checked:declaration" name="chkcompdeclaration" id="chkcompdeclaration" value="true" />
                                                        <label for="chkcompdeclaration">I Declare that this company is special purpose company as defined by the corporations act Regulations 2003-Reg 3</label>
                                                    </div>
                                                    <div class="clearfix"></div>
                                                    <span class="validationMessage" data-bind="validationMessage: declaration"></span>
                                                </div>
                                            </div>

                                            <div class="col-md-6" style="display:none;">
                                                <h5>Class of Company</h5>
                                                <select name="ddcompfor" id="ddcompclass">
                                                    <option value="">Select</option>
                                                    <option value="smsf">Limited by Share</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6" style="display:none;">
                                                <h5>Select Special Purpose(if any)</h5>
                                                <select name="ddcompfor" id="ddcompsubclass">
                                                    <option value="0">--select--</option>
                                                    <option value="Hunt">home unit (HUNT)</option>
                                                    <option value="pstc">superannuation trustee (PSTC)</option>
                                                    <option value="pnpc">charitable purposes only (PNPC)</option>
                                                    <option value="prop">PROPRIETARY (PROP)</option>
                                                </select>
                                            </div>
                                        </div>

                                        <div class="row with-forms">
                                            <div class="col-md-6">
                                                <h5>Which State or Territory would you like your company registered in?</h5>
                                                <select name="ddcompregstate" id="ddcompregstate" data-bind="value:state">
                                                    <option value="">Select state</option>
                                                    <option value="ACT">Australian Capital Territory</option>
                                                    <option value="NSW">New South Wales</option>
                                                    <option value="NT">Northern Territory</option>
                                                    <option value="QLD">Queensland</option>
                                                    <option value="SA">South Australia</option>
                                                    <option value="TAS">Tasmania</option>
                                                    <option value="VIC">Victoria</option>
                                                    <option value="WA">Western Australia</option>
                                                </select>
                                            </div>
                                        </div>

                                        <hr />
                                        <div class="row with-forms" data-bind="visible:$data.companyuse()=='smsf'" style="background-color: rgb(255, 244, 182); padding: 21px;">
                                            <div class="col-md-12">
                                                <div class="checkboxes">
                                                    @*<input type="checkbox" name="chkUlimateHoldingCompany" id="chkUlimateHoldingCompany" value="yes" data-bind="checked:UlimateHoldingCompany,event:{change:uhctoggle.bind($data, namuna ? 'yes' : 'no' ),click:ssss}" />*@
                                                    <input type="checkbox" name="chkUlimateHoldingCompany" id="chkUlimateHoldingCompany" data-bind="checked: someBoolProperty" />

                                                    <label for="chkUlimateHoldingCompany">Will the company have an <b>Ultimate Holding Company </b> upon registration?</label>
                                                </div>
                                            </div>
                                            <div class="col-md-12" data-bind="visible:pnlshowultimate" style="background-color: rgb(255, 244, 182);"> @* pnlshowultimate*@
                                                <div class="col-md-6">
                                                    <h5>Company name</h5>
                                                    <input name="ucompanyname" id="ucompanyname" type="text" data-bind="value:ucompanyname" />
                                                </div>
                                                <div class="col-md-6">  </div>   
                                                <div class="col-md-6">
                                                    <h5>ABN</h5>
                                                    <input name="acnarbnabn" id="acnarbnabn" type="text" data-bind="value:acnarbnabn" />
                                                </div>
                                                <div class="col-md-6">
                                                    <h5>Country of incorporation(if not Australia)</h5>
                                                    <input name="countryIcor" id="countryIcor" type="text" data-bind="value:countryIcor" />
                                                </div>
                                            </div>
                                        </div>

                                        <hr />

                                        <div class="row with-forms">
                                            <div class="col-md-12">
                                                <h4>If this company is trustee for a trust add details of this entity</h4>
                                                <h3>Trust details</h3>
                                            </div>

                                            <div class="col-md-6">
                                                <h5>Trust name</h5>
                                                <input name="trustname" id="trustname" type="text" data-bind="value:trustname" />
                                            </div>
                                            <div class="col-md-6">
                                                <h5>ABN</h5>
                                                <input name="trustABN" id="trustABN" type="text" placeholder="Optional" data-bind="value:ABN" />
                                            </div>
                                            <div class="col-md-6">
                                                <h5>TFN</h5>
                                                <input name="trustTFN" id="trustTFN" type="text" placeholder="Optional" data-bind="value:TFN" />
                                            </div>
                                            <div class="col-md-6">
                                                <h5>Trust address</h5>
                                                <input name="trustaddress" id="trustaddress" type="text" data-bind="value:trustaddress" />
                                            </div>
                                            <div class="col-md-6">
                                                <h5>Country Of Incorporation</h5>
                                                <input name="trustcountry" id="trustcountry" type="text" readonly="readonly" disabled="disabled" data-bind="value:trustcountry" />
                                            </div>
                                        </div>

                                        <div id="result-modal" style="display:none" data-bind="visible:resultbinded()">
                                            <div class="container">
                                                <div class="row">
                                                    <div class="col-md-6 col-md-offset-3 bg-white padding-top-25 padding-bottom-25">
                                                        <h3>Search result - <span data-bind="text:companyName" class="text-uppercase"></span></h3>
                                                        <div class="notification" data-bind="css:resclass">
                                                            <h4 data-bind="text:stats"></h4>
                                                            <p data-bind="text:desc"></p>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                        <hr />
                                                        <div class="pull-right">
                                                            <button type="button" class="button border" data-bind="click:closeModal">Close</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="divider"></div>
                                    @{ if (ViewBag.cn == null || ViewBag.cn == "")
                                        {
                                            <button type="submit" class="button preview pull-right margin-bottom-15" data-bind="enable:estatus()">
                                                Continue
                                                <i class="fa fa-arrow-circle-right"></i>
                                            </button>
                                        }
                                        else

                                        {
                                            <button type="submit" class="button preview pull-right margin-bottom-15">
                                                Continue
                                                <i class="fa fa-arrow-circle-right"></i>
                                            </button>
                                        }

                                    }
                                </form>
                            </div>
                    <!-- company detail-->
                    <!-- company address detail-->
                                        <div class="tab-content " data-bind="css:tabbing()==3?'tab-content active':'tab-content'">

                                            <form id="companyaddressform" class="submit-page p-0" data-bind="submit:companyaddsubmit">

                                                <h3>Company address details</h3>
                                                <em>If the company is acting as a trustee you can use your Home Address</em>

                                                <div class="submit-section" data-bind="with:companyAddress()">
                                                    <input type="text" id="txtAutocomplete1" style="width: 300px;color:red;display:none;" placeholder="Enter your address" /><br /><br />
                                                    <div class="row with-forms">
                                                        <div class="col-md-6">
                                                            <h5>Unit / Level(Optional)</h5>
                                                            <input type="text" name="compaddunit" id="compaddunit" data-bind="value:companyAddress().unit" />
                                                        </div>
                                                        <div class="col-md-6">
                                                            <h5>Street address</h5>
                                                            <input type="text" name="compaddstreet" id="compaddstreet" data-bind="value:companyAddress().street" />
                                                        </div>
                                                        <div class="col-md-4">
                                                            <h5>State</h5>
                                                            <select name="ddcompaddstate" id="ddcompaddstate" data-bind="value:companyAddress().state">
                                                                <option value="">Select state</option>
                                                                <option value="ACT">Australian Capital Territory</option>
                                                                <option value="NSW">New South Wales</option>
                                                                <option value="NT">Northern Territory</option>
                                                                <option value="QLD">Queensland</option>
                                                                <option value="SA">South Australia</option>
                                                                <option value="TAS">Tasmania</option>
                                                                <option value="VIC">Victoria</option>
                                                                <option value="WA">Western Australia</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <h5>Suburb</h5>
                                                            <input type="text" name="compaddsuburb" id="compaddsuburb" data-bind="value:companyAddress().suburb" />
                                                        </div>
                                                        <div class="col-md-4">
                                                            <h5>Postcode</h5>
                                                            <input type="text" name="compaddpostcode" id="compaddpostcode" data-bind="value:companyAddress().postcode" />
                                                        </div>
                                                    </div>

                                                    <div class="row with-forms">

                                                        <div class="col-md-12">
                                                            <h5>Is the Principal place of business the same as the registered office? and will the company operate from this address?and the home address of director of company?</h5>

                                                            <div class="checkboxes margin-top-15 margin-right-15">
                                                                <input type="radio" value="yes" name="rdosamecompadd" id="rdosamecompadd1" data-bind="checked:companyAddress().sameadd,event:{change:patoggle.bind($data,'yes')}" />
                                                                <label for="rdosamecompadd1">Yes</label>
                                                            </div>
                                                            <div class="checkboxes margin-top-15">
                                                                <input type="radio" value="no" name="rdosamecompadd" id="rdosamecompadd2" data-bind="checked:companyAddress().sameadd,event:{change:patoggle.bind($data,'no')}" />
                                                                <label for="rdosamecompadd2">No</label>
                                                            </div>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                        <div style="display:none;background-color:#fff4b6;padding:20px;" data-bind="visible:paddpnl,with:companyPrincipalAddress()">
                                                            <h4>Principle place of business</h4>
                                                            <div class="row with-forms">
                                                                <div class="col-md-6">
                                                                    <h5>Unit / Level(Optional)</h5>
                                                                    <input type="text" name="compaddunit_p" id="compaddunit_p" data-bind="value:companyPrincipalAddress().unit" />
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <h5>Street address</h5>
                                                                    <input type="text" name="compaddstreet_p" id="compaddstreet_p" data-bind="value:companyPrincipalAddress().street" placeholder="PO Boxes and Locked bag addresses are not allowed." />
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <h5>State</h5>
                                                                    <select name="ddcompaddstate_p" id="ddcompaddstate_p" data-bind="value:companyPrincipalAddress().state">
                                                                        <option value="">Select state</option>
                                                                        <option value="ACT">Australian Capital Territory</option>
                                                                        <option value="NSW">New South Wales</option>
                                                                        <option value="NT">Northern Territory</option>
                                                                        <option value="QLD">Queensland</option>
                                                                        <option value="SA">South Australia</option>
                                                                        <option value="TAS">Tasmania</option>
                                                                        <option value="VIC">Victoria</option>
                                                                        <option value="WA">Western Australia</option>
                                                                    </select>
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <h5>Suburb</h5>
                                                                    <input type="text" name="compaddsuburb_p" id="compaddsuburb_p" data-bind="value:companyPrincipalAddress().suburb" />
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <h5>Postcode</h5>
                                                                    <input type="text" name="compaddpostcode_p" id="compaddpostcode_p" data-bind="value:companyPrincipalAddress().postcode" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="divider"></div>
                                                <button type="submit" class="button preview pull-right margin-bottom-15">
                                                    Continue
                                                    <i class="fa fa-arrow-circle-right"></i>
                                                </button>
                                            </form>
                                        </div>
                    <!-- company address detail-->
                    <!-- director detail-->
                                        <div class="tab-content " data-bind="css:tabbing()==4?'tab-content active':'tab-content'">
                                            <form id="directordetailform" class="submit-page p-0" data-bind="submit:directorsubmit">
                                                <h3>Director details</h3>

                                                <div class="submit-section">
                                                    <div class="row with-forms">
                                                        <div class="col-md-6">
                                                            <h5>Number of Directors</h5>
                                                            <select id="dddirectorno" name="dddirectorno" data-bind="value:directorsCounter">
                                                                <option value="1">1</option>
                                                                <option value="2">2</option>
                                                                <option value="3">3</option>
                                                                <option value="4">4</option>
                                                                <option value="5">5</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <hr />

                                                    <div data-bind="foreach:directors">
                                                        <input class="dirid" type="text" name="directorid" data-bind="value:id" style="display:none;" />
                                                        <h4 class="margin-bottom-20">Officeholder details <span data-bind="value:$index"></span></h4>

                                                        <div class="clearfix"></div>
                                                        <div class="row with-forms">
                                                            <div class="col-md-6">
                                                                <h5>1st Given Name,2nd Given Name</h5>
                                                                <input class="dirfname" type="text" name="directorfname" data-bind="value:fname" />
                                                            </div>
                                                            <div class="col-md-6">
                                                                <h5>Family name</h5>
                                                                <input type="text" name="directorlname" id="directorlname" data-bind="value:lname" />
                                                            </div>
                                                        </div>

                                                        <div class="row with-forms">
                                                            <div class="col-md-6">

                                                                <h5>Date of birth</h5>
                                                                <div class="row">
                                                                    <div class="col-sm-4 col-md-4">
                                                                        <select name="directordobday" id="directordobday" data-bind="value:dobday">
                                                                            <option value="">Day</option>
                                                                            <option value="1" label="1">1</option>
                                                                            <option value="2" label="2">2</option>
                                                                            <option value="3" label="3">3</option>
                                                                            <option value="4" label="4">4</option>
                                                                            <option value="5" label="5">5</option>
